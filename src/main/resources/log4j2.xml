<configuration>
    <Appenders>
        <Console name="Console" target="SYSTEM_OUT">
            <PatternLayout pattern="%d{HH:mm:ss.SSS} [%t] %-5level %logger{36} - %msg%n"/>
        </Console>
    </Appenders>
    <Loggers>
      <Root level="warn">
        <AppenderRef ref="Console"/>
      </Root>
      <!--<Logger name="org.apache.cayenne" level="trace" additivity="false">
        <AppenderRef ref="Console"/>
      </Logger>-->
    </Loggers> 
</configuration>