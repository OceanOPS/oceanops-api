<jaxb:bindings 
    xmlns:xs="http://www.w3.org/2001/XMLSchema"
    xmlns:jaxb="http://java.sun.com/xml/ns/jaxb"
    xmlns:xjc="http://java.sun.com/xml/ns/jaxb/xjc"
    xmlns:namespace="http://jaxb2-commons.dev.java.net/namespace-prefix"
    jaxb:extensionBindingPrefixes="xjc"
    version="2.1">

	<jaxb:globalBindings generateIsSetMethod="true" fixedAttributeAsConstantProperty="true">
        <xjc:serializable uid="1" />
    </jaxb:globalBindings>

	<jaxb:bindings schemaLocation="wmdr.xsd">
		<jaxb:bindings node="//xs:complexType[@name='AbstractEnvironmentalMonitoringFacilityType']//xs:element[@name='description']">
		  <jaxb:property name="descriptionList"/>
		</jaxb:bindings> 
        <jaxb:bindings>
            <namespace:prefix name="wmdr" />
            <!-- Additional @XmlNs prefix declarations to take effect for this schema/package -->
            <namespace:prefix name="xsi" namespaceURI="http://www.w3.org/2001/XMLSchema-instance" />
            <namespace:prefix name="xlink" namespaceURI="http://www.w3.org/1999/xlink" />
            <namespace:prefix name="gmd" namespaceURI="http://www.isotc211.org/2005/gmd" />
            <namespace:prefix name="gco" namespaceURI="http://www.isotc211.org/2005/gco" />
            <namespace:prefix name="om" namespaceURI="http://www.opengis.net/om/2.0" />
            <namespace:prefix name="gml" namespaceURI="http://www.opengis.net/gml/3.2" />
            <namespace:prefix name="sam" namespaceURI="http://www.opengis.net/sampling/2.0" />
            <namespace:prefix name="sams" namespaceURI="http://www.opengis.net/samplingSpatial/2.0" />
            <namespace:prefix name="metce" namespaceURI="http://def.wmo.int/metce/2013" />
            <namespace:prefix name="opm" namespaceURI="http://def.wmo.int/opm/2013" />
            <namespace:prefix name="gts" namespaceURI="http://www.isotc211.org/2005/gts" />
            <namespace:prefix name="gmx" namespaceURI="http://www.isotc211.org/2005/gmx" />
        </jaxb:bindings>
	</jaxb:bindings> 
</jaxb:bindings>